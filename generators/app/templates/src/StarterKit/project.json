{
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:51002",
        "kestrel": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:51003",
        "ef": "EntityFramework.Commands"
    },
	"dependencies": {
		"AutoMapper": "3.3.0",
		"EntityFramework": "6.1.2",
		"Npgsql": "2.2.4.1",
		"Npgsql.EntityFramework": "2.2.4.1",
        "Microsoft.AspNet.Diagnostics": "1.0.0-beta6",
        "Microsoft.AspNet.Hosting":  "1.0.0-beta6", 
        "Microsoft.AspNet.Http": "1.0.0-beta6",
        "Microsoft.AspNet.Http.Extensions": "1.0.0-beta6",
		"Microsoft.AspNet.Mvc": "6.0.0-beta6",
		"Microsoft.AspNet.Server.IIS": "1.0.0-beta6",
		"Microsoft.AspNet.Server.Kestrel": "1.0.0-beta6",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta6",
		"Microsoft.AspNet.StaticFiles": "1.0.0-beta6",
		"Microsoft.AspNet.WebUtilities": "1.0.0-beta6", 
		"Microsoft.Framework.Configuration": "1.0.0-beta6",
		"Microsoft.Framework.Configuration.Json": "1.0.0-beta6",
		"Microsoft.Framework.DependencyInjection": "1.0.0-beta6",
		"Microsoft.Framework.Logging": "1.0.0-beta6",
		"Microsoft.Framework.Logging.NLog": "1.0.0-beta6",
		"Microsoft.Framework.OptionsModel": "1.0.0-beta6",
		"Kestrel": "1.0.0-beta6",
		"System.Linq.Dynamic": "1.0.4.0"
	},
    "frameworks" : {
        "dnx451" : { 
       		"frameworkAssemblies": {
                "System.ComponentModel.DataAnnotations": "",
				"System.Net.Http": "4.0.0.0",
				"System.Data": "4.0.0.0"
            }
        }
    },
	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components",
		".grunt"
	],
    "publishExclude": [
        "node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
    ]
