{
  "name": "StarterKit",
  "private": true,
  "dependencies": {
    "bootstrap": "latest",
    "angular": "~1.4.3",
    "angular-animate": "~1.4.3",
    "angular-mocks": "~1.4.3",
    "angular-resource": "~1.3.15",
    "angular-route": "~1.4.3",
    "angular-ui-router": "~0.2.14",
    "angular-local-storage": "~0.1.5",
    "jquery": "~2.1.1",
    "jquery-validation": "~1.13.1",
    "jquery-validation-unobtrusive": "~3.2.2",
    "font-awesome": "latest",
    "detect-element-resize": "~0.5.3",
    "modernizr": "~2.8.3",
    "respond": "~1.4.2",
    "slimscroll": "~1.3.3",
    "underscore": "~1.8.2",
    "uri.js": "~1.15.0"
  },
  "exportsOverride": {
    "bootstrap": {
      "css": "dist/css/*",
      "js": "dist/js/*",
      "fonts": "dist/fonts/*"
    },
    "angular": {
      "js": "*.js"
    },
    "angular-animate": {
      "js": "*.js"
    },
    "angular-mocks": {
      "js": "*.js"
    },
    "angular-resource": {
      "js": "*.js"
    },
    "angular-route": {
      "js": "*.js"
    },
    "angular-ui-router": {
      "js": "release/*.js"
    },
    "angular-local-storage": {
      "js": "dist/*.js"
    },
    "jquery": {
      "js": "dist/*.js"
    },
    "jquery-validation": {
      "js": "dist/*.js"
    },
    "jquery-validation-unobtrusive": {
      "js": "*.js"
    },
    "font-awesome": {
      "css": "css/*",
      "fonts": "fonts/*"
    },
    "detect-element-resize": {
      "js": "*.js"
    },
    "modernizr": {
      "js": "modernizr.js"
    },
    "respond": {
      "js": "dest/*.min.js"
    },
    "slimscroll": {
      "js": "*.js"
    },
    "underscore": {
      "js": "underscore.js"
    },
    "uri.js": {
      "js": "src/URI.js"
    }
  },
  "resolutions": {
    "angular": ">= 1.0.8"
  }
}
