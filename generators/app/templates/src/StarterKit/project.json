{
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "commands": {
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:51002;https://localhost:51003",
        "ef": "EntityFramework.Commands"
    },
	"dependencies": {
        "Microsoft.AspNet.Cors": "6.0.0-rc1-final",
        "Microsoft.AspNet.Diagnostics": "1.0.0-rc1-final",
		"Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
		"Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1-final",
		"Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
        "Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final",
		"Microsoft.AspNet.WebUtilities": "1.0.0-rc1-final", 
		"Microsoft.Framework.Configuration": "1.0.0-rc1-final",
		"Microsoft.Framework.Configuration.Json": "1.0.0-rc1-final",
		"Microsoft.Framework.DependencyInjection": "1.0.0-rc1-final",
		"Microsoft.Framework.Logging": "1.0.0-rc1-final",
		"Microsoft.Framework.Logging.NLog": "1.0.0-rc1-final",
		"Microsoft.Framework.OptionsModel": "1.0.0-rc1-final",
		"System.Linq.Dynamic": "1.0.4.0"
	},
    "frameworks" : {
        "dnx451" : { 
       		"frameworkAssemblies": {
                "System.ComponentModel.DataAnnotations": "",
				"System.Net.Http": "4.0.0.0",
				"System.Data": "4.0.0.0"
            }
        }
    },
    "compilationOptions": {
        "emitEntryPoint": true
    },
	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components",
		".grunt"
	],
    "publishExclude": [
        "node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
    ]
}
