{
    "ConsoleLogging": 
    {
        "IncludeScopes": false,
        "LogLevel": 
        {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
	"SystemLog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"System": "Information",
				"Microsoft": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "LiterateConsole"
			},
			{
				"Name": "Elasticsearch",
				"Args": {
					"nodeUris": "http://localhost:9200",
					"indexFormat": "logstash-system-starterkit-{0:yyyy.MM.dd}",
					"templateName": "starterkit-template",
					"minimumLogEventLevel": "Information",
					"batchPostingLimit": 50,
					"batchBetweenPeriod": 2000,
					"typeName": "SystemLogEvent",
					"bufferBaseFilename": "C:/Temp/LogDigipolis/starterkit-buffer-system"
				}
			}
		],
		"Enrich": [ "FromLogContext" ]
	},
	"ApplicationLog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"System": "Information",
				"Microsoft": "Information"
			}
		},
		"WriteTo": [
			{
				"Name": "Elasticsearch",
				"Args": {
					"nodeUris": "http://localhost:9200",
					"indexFormat": "logstash-application-starterkit-{0:yyyy.MM.dd}",
					"templateName": "starterkit-template",
					"minimumLogEventLevel": "Information",
					"batchPostingLimit": 50,
					"batchBetweenPeriod": 2000,
					"typeName": "AppLogEvent",
					"bufferBaseFilename": "C:/Temp/LogDigipolis/starterkit-buffer-application"
				}
			}
		],
		"Enrich": [ "FromLogContext" ]
	}
}