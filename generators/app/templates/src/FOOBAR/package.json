{
  "name": "foobar",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng lint && ng build --configuration production && del-cli ./wwwroot/index.html && move-cli ./wwwroot/manifest.webmanifest ./wwwroot/manifest.json",
    "build:dev": "ng build --source-map && del-cli ./wwwroot/index.html && move-cli ./wwwroot/manifest.webmanifest ./wwwroot/manifest.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "swagger:generate": "del-cli ClientApp/app/models/api && mkdirp ./ClientApp/app/shared/models/api && swagger-angular-generator --no-store -s ../swagger/spec.json -d ./ClientApp/app/shared/models/api && for directory in common controllers store; do del-cli ./ClientApp/app/shared/models/api/${directory}; done",
    "compodoc:generate": "./node_modules/.bin/compodoc -p ClientApp/tsconfig.app.json -n \"LEZ documentation\"",
    "compodoc:serve": "./node_modules/.bin/compodoc -s ClientApp/tsconfig.app.json",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@a-ui/core": "^5.0.2",
    "@acpaas-ui/ngx-layout": "^5.2.1",
    "@acpaas-ui/ngx-logo": "^5.0.0",
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "tslint-etc": "^1.13.10",
    "zone.js": "~0.11.4",
    "@angular/service-worker": "13.3.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "del-cli": "^3.0.1",
    "eslint": "^8.12.0",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "move-cli": "^1.2.1",
    "node-sass": "^7.0.1",
    "swagger-angular-generator": "^4.2.2",
    "typescript": "~4.6.2"
  }
}
